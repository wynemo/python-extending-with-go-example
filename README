import newmath
import base64
a = ''
b = base64.b64encode("202009081234+张三+2020-09-08 14:51:39+2020-09-29 00:00:00+1.3.2.1")
c = "/5Vyd/K38gpPSHodvrvInO8iuf9j9ptTx8wQRg47FsGx/Pv6Rg13wAsqjBRRtDdeFUuA63ZlOjrtMN32a8ngRQ=="
newmath.sum_qqq(a, b, c)
